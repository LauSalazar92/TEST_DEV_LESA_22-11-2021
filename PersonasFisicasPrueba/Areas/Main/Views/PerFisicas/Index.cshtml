
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">

    <div class="col-6">
        <h2>Listado de Personas Fisicas</h2>
    </div>

    <div class="col-6">
        <a asp-area="Main" asp-controller="PerFisicas" asp-action="Create" class="btn btn-success">
            Agregar Persona Fisica
        </a>
    </div>

</div>





<table id="tblPersona" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            <th>IdPersonaFisica</th>
            <th>Fecha de Registro</th>
            <th>Fecha Actualizacion</th>
            <th>Nombre completo</th>
            <th>RFC</th>
            <th>FechaNacimiento</th>
            <th>Usuario Agregado</th>
         
            <th>Acciones</th>
        </tr>
    </thead>
</table>

@section Scripts{


    <script src="~/js/persona.js"></script>
}
